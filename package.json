{
  "name": "@grisaiaevy/crafting-agent",
  "version": "0.0.2",
  "description": "A TypeScript library for building AI agents with support for multiple LLM providers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "anthropic",
    "openai",
    "google",
    "typescript",
    "streaming",
    "tools"
  ],
  "author": "Crafting Agent",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kgmemory/CraftingAgent"
  },
  "bugs": {
    "url": "https://github.com/kgmemory/CraftingAgent/issues"
  },
  "homepage": "https://github.com/kgmemory/CraftingAgent#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@google/genai": "^1.28.0",
    "@openrouter/sdk": "^0.1.27",
    "nanoid": "^5.1.6",
    "openai": "^6.10.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "typescript": "^5.7.3"
  }
